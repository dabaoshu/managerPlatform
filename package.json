{
  "name": "data-shu",
  "version": "0.1.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "stage": "cross-env NODE_ENV=stage NODE_ENV=stage COMPRESS=none NODE_OPTIONS=--max_old_space_size=5120 umi build",
    "deploy": "npm run build && npm run gh-pages",
    "dev": "npm run start:dev",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "start": "cross-env UMI_UI=1 umi dev",
    "start:dev": "cross-env NODE_ENV=dev MOCK=none UMI_ENV=dev umi dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev umi dev",
    "start:no-ui": "cross-env UMI_UI=none UMI_ENV=dev umi dev",
    "start:stage": "cross-env NODE_ENV=stage UMI_ENV=dev umi dev",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/charts": "^1.4.2",
    "@ant-design/icons": "^4.7.0",
    "@ant-design/moment-webpack-plugin": "^0.0.4",
    "@ant-design/pro-components": "2.6.28",
    "@antv/data-set": "^0.11.0",
    "@stomp/stompjs": "^7.0.0",
    "@umijs/route-utils": "^2.0.0",
    "ahooks": "^3.7.2",
    "antd": "^5.9.4",
    "classnames": "^2.3.0",
    "html-webpack-plugin": "^5.5.3",
    "js-md5": "^0.8.0",
    "lodash": "^4.17.0",
    "lodash-decorators": "^6.0.0",
    "mitt": "^3.0.0",
    "moment": "^2.29.0",
    "monaco-editor": "^0.44.0",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "node-jsencrypt": "^1.0.0",
    "numeral": "^2.0.6",
    "nzh": "^1.0.3",
    "rc-menu": "^9.1.0",
    "rc-util": "^5.16.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-iframe": "^1.8.5",
    "react-monaco-editor": "^0.54.0",
    "react-router": "^4.3.1",
    "umi": "^3.5.41"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.1.0",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.0",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.0",
    "@types/react": "^18.2.27",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/stompjs": "^2.3.5",
    "@umijs/fabric": "^2.8.0",
    "@umijs/plugin-blocks": "^2.2.0",
    "@umijs/plugin-esbuild": "^1.4.0",
    "@umijs/plugin-qiankun": "^2.41.0",
    "@umijs/preset-ant-design-pro": "^1.3.0",
    "@umijs/preset-dumi": "^1.1.0",
    "@umijs/preset-react": "^2.1.0",
    "@umijs/preset-ui": "^2.2.9",
    "cross-env": "^7.0.3",
    "cross-port-killer": "^1.3.0",
    "detect-installer": "^1.0.0",
    "eslint": "^7.32.0",
    "gh-pages": "^3.2.0",
    "husky": "^7.0.4",
    "jsdom-global": "^3.0.0",
    "lint-staged": "^10.0.0",
    "mockjs": "^1.1.0",
    "prettier": "^2.5.0",
    "stylelint": "^13.0.0",
    "typescript": "^4.5.0"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
